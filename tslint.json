{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [
      false
    ],
    "no-console": [
      false
    ],
    "max-line-length": [
      true,
      {
        "limit": 160,
        "ignore-pattern": "^import [^,]+ from"
      }
    ]
  },
  "rulesDirectory": []
}