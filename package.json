{
  "name": "dank-times-bot",
  "version": "1.3.2",
  "description": "This Telegram bot keeps track of 'dank' times such as 13:37 and 04:20, and awards points to users who call them out.",
  "main": "built/main.js",
  "scripts": {
    "clean": "rimraf ./built",
    "prestart": "npm run clean && tsc",
    "start": "node ./built/main.js",
    "pretest": "npm run clean && tsc",
    "test": "mocha $(find built -name '*.spec.js')",
    "lint": "tslint -p tsconfig.json --fix"
  },
  "author": "Agadar",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/Agadar/DankTimesBot.git"
  },
  "dependencies": {
    "cron": "^1.2.1",
    "moment-timezone": "^0.5.13",
    "node-cleanup": "^2.1.2",
    "node-telegram-bot-api": "^0.27.1"
  },
  "devDependencies": {
    "@types/chai": "^4.0.3",
    "@types/cron": "^1.2.1",
    "@types/mocha": "^2.2.41",
    "@types/moment-timezone": "^0.2.34",
    "@types/node": "^8.0.19",
    "chai": "^4.1.1",
    "mocha": "^3.5.0",
    "rimraf": "^2.6.2"
  }
}
