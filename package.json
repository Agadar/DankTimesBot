{
  "name": "dank-times-bot",
  "version": "2.0.0-alpha",
  "description": "This Telegram bot keeps track of 'dank' times such as 13:37 and 04:20, and awards points to users who call them out.",
  "main": "built/main.js",
  "scripts": {
    "clean": "rimraf ./built",
    "prestart": "npm run clean && tsc",
    "start": "node ./built/main.js",
    "pretest": "npm run clean && tsc",
    "test": "mocha ./built/**/*.spec.js",
    "lint": "tslint -p tsconfig.json --fix"
  },
  "author": "Agadar",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/Agadar/DankTimesBot.git"
  },
  "dependencies": {
    "cron": "^1.8.2",
    "moment-timezone": "^0.5.33",
    "node-cleanup": "^2.1.2",
    "node-telegram-bot-api": "^0.54.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/cron": "^1.7.3",
    "@types/mocha": "^9.0.0",
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "^14.17.6",
    "@types/node-cleanup": "^2.1.2",
    "@types/node-telegram-bot-api": "^0.51.4",
    "chai": "^4.3.4",
    "mocha": "^9.1.1",
    "rimraf": "^3.0.2",
    "ts-mockito": "^2.6.1",
    "tslint": "^5.20.1",
    "typescript": "^4.4.2"
  }
}
